{
  "name": "shop",
  "version": "0.0.1",
  "description": "Shop Application - Open Marketplace Application",
  "author": "huhn511",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "generate:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy:gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "deploy": "npm run generate:gh-pages && npm run deploy:gh-pages"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@tryghost/content-api": "^1.3.8",
    "element-ui": "^2.4.11",
    "express": "^4.16.4",
    "nuxt": "^2.0.0",
    "nuxt-i18n": "^6.6.1",
    "nuxt-leaflet": "0.0.21",
    "vue-iota-payment": "0.0.31"
  },
  "devDependencies": {
    "cross-env": "^5.2.1",
    "node-sass": "^7.0.0",
    "nodemon": "^1.18.9",
    "push-dir": "^0.4.1",
    "sass-loader": "^8.0.2"
  }
}
